-- ================= TAB OTOMATISASI (FARM, RAID, SEA) =================
local TabCombat = Window:CreateTab("Combat & Farm", 4483362458)
TabCombat:CreateSection("Fitur Pembantaian")
TabCombat:CreateToggle({Name = "Auto Farm Level", CurrentValue = false, Callback = function(v) end})
TabCombat:CreateToggle({Name = "Kill Aura (Sangar)", CurrentValue = false, Callback = function(v) end})

local TabExtra = Window:CreateTab("Raid & Sea", 4483362458)
TabExtra:CreateSection("Dungeon & Sea Event")
TabExtra:CreateButton({Name = "Auto Start Raid", Callback = function() end})
TabExtra:CreateToggle({Name = "Auto Sea Beast", CurrentValue = false, Callback = function(v) end})

-- Notifikasi Akhir
Rayfield:Notify({
   Title = "Hamba Siap Melayani",
   Content = "Menu Teleportasi Terstruktur Telah Siap, Yang Mulia.",
   Duration = 5,
   Image = 4483362458,
})

